{
  "extends": "nx/presets/npm.json",
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "affected": {
    "defaultBase": "master"
  },
  "targetDefaults": {
    "build": {
      "cache": true,
      "dependsOn": [
        "^build"
      ]
    },
    "lint": {
      "cache": true
    },
    "e2e": {
      "cache": true
    }
  },
  "nxCloudAccessToken": "Mjc5ZDMwMzgtOTdmMy00MTdkLTk3NjUtZDliYjE3YjViNjI1fHJlYWQtd3JpdGU=",
  "generators": {
    "@nx/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "none",
      "strict": true,
      "prefix": "uni",
      "standalone": true,
      "bundler": "esbuild",
      "ssr": false
    },
    "@nx/angular:library": {
      "linter": "eslint",
      "unitTestRunner": "jest",
      "style": "scss",
      "strict": true,
      "prefix": "uni",
      "buildable": true,
      "publishable": true,
      "standalone": true,
      "projectNameAndRootFormat": "derived",
      "simpleName": true,
      "skipModule": true,
      "changeDetection": "OnPush",
      "flat": true
    },
    "@nx/angular:component": {
      "style": "scss",
      "standalone": true,
      "prefix": "uni",
      "changeDetection": "OnPush"
    }
  }
}
